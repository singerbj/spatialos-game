{
  "workerType": "JavaWorker",
  "layer": "java",
  "entityInterest": {
    "rangeEntityInterest": {
      "radius": 2
    }
  },
  "componentDelivery": {
    "default": "RELIABLE_ORDERED",
    "checkoutAllInitially": true
  },
  "launchConfiguration": {
    "cloudDeployment": {
      "linux": {
        "localBuildDirectory": "../JavaWorker/build/",
        "command": "java",
        "arguments": [
            "-jar",
            "build/libs/JavaWorker.jar",
            "${IMPROBABLE_RECEPTIONIST_HOST}",
            "${IMPROBABLE_RECEPTIONIST_PORT}",
            "${IMPROBABLE_WORKER_ID}"
        ]
      }
    },
    "localDeployment": {
      "windows": {
        "localBuildDirectory": "../JavaWorker/build/",
        "command": "java",
        "arguments": [
            "-jar",
            "build/libs/JavaWorker.jar",
            "${IMPROBABLE_RECEPTIONIST_HOST}",
            "${IMPROBABLE_RECEPTIONIST_PORT}",
            "${IMPROBABLE_WORKER_ID}"
        ]
      },
      "macos": {
        "localBuildDirectory": "../JavaWorker/build/",
        "command": "java",
        "arguments": [
            "-jar",
            "build/libs/JavaWorker.jar",
            "${IMPROBABLE_RECEPTIONIST_HOST}",
            "${IMPROBABLE_RECEPTIONIST_PORT}",
            "${IMPROBABLE_WORKER_ID}"
        ]
      },
      "linux": {
        "localBuildDirectory": "../JavaWorker/build/",
        "command": "java",
        "arguments": [
            "-jar",
            "build/libs/JavaWorker.jar",
            "${IMPROBABLE_RECEPTIONIST_HOST}",
            "${IMPROBABLE_RECEPTIONIST_PORT}",
            "${IMPROBABLE_WORKER_ID}"
        ]
      }
    }
  }
}
